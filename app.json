{
  "expo": {
    "name": "FoodAnnotation",
    "slug": "goodfood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#29C752"
    },
    "assetBundlePatterns": [
      "./assets/fonts/"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon2.png",
        "backgroundColor": "#29C752"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.tokoss.goodfood"
    },
    "web": {
      "favicon": "./assets/icon2.png",
      "bundler": "metro",
      "backgroundColor": "#29C752"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "aa6dd71c-49c5-430e-b86f-09ed36e72d1f"
      }
    },
    "owner": "tokoss"
  }
}
